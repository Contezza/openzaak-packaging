<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<servers>
		<server>
			<id>cntz-activiti-private</id>
			<username>${env.CNTZ_GITLAB_NEXUS_USER}</username>
			<password>${env.CNTZ_GITLAB_NEXUS_PASSWD}</password>
		</server>
		<server>
			<id>cntz-activiti-public</id>
			<username>${env.CNTZ_GITLAB_NEXUS_USER}</username>
			<password>${env.CNTZ_GITLAB_NEXUS_PASSWD}</password>
		</server>
		<server>
			<id>cntz-alfresco-public</id>
			<username>${env.CNTZ_GITLAB_NEXUS_USER}</username>
			<password>${env.CNTZ_GITLAB_NEXUS_PASSWD}</password>
		</server>
		<server>
			<id>cntz-alfresco-public-snapshots</id>
			<username>${env.CNTZ_GITLAB_NEXUS_USER}</username>
			<password>${env.CNTZ_GITLAB_NEXUS_PASSWD}</password>
		</server>
		<server>
			<id>cntz-alfresco-private</id>
			<username>${env.CNTZ_GITLAB_NEXUS_USER}</username>
			<password>${env.CNTZ_GITLAB_NEXUS_PASSWD}</password>
		</server>
		<server>
			<id>mvnrepository</id>
			<username>${env.CNTZ_GITLAB_NEXUS_USER}</username>
			<password>${env.CNTZ_GITLAB_NEXUS_PASSWD}</password>
		</server>
		<server>
			<id>releases</id>
			<username>${env.CNTZ_GITLAB_NEXUS_USER}</username>
			<password>${env.CNTZ_GITLAB_NEXUS_PASSWD}</password>
		</server>
		<server>
			<id>snapshots</id>
			<username>${env.CNTZ_GITLAB_NEXUS_USER}</username>
			<password>${env.CNTZ_GITLAB_NEXUS_PASSWD}</password>
		</server>
		<server>
			<id>thirdparty</id>
			<username>${env.CNTZ_GITLAB_NEXUS_USER}</username>
			<password>${env.CNTZ_GITLAB_NEXUS_PASSWD}</password>
		</server>
		<server>
			<id>alfresco-private-repository</id>
			<username>${env.CNTZ_ALFRESCO_NEXUS_USER}</username>
			<password>${env.CNTZ_ALFRESCO_NEXUS_PASSWD}</password>
		</server>
		<!-- Customer Secret-Items -->
		<server>
			<id>secret-items</id>
			<username>${env.CNTZ_GITLAB_NEXUS_USER}</username>
			<password>${env.CNTZ_GITLAB_NEXUS_PASSWD}</password>
		</server>
		<!-- GIT credentials -->
		<server>
			<id>git.contezza.nl</id>
			<username>${env.CNTZ_GIT_OAUTH_USER}</username>
			<password>${env.CNTZ_GIT_OAUTH_PASSWD}</password>
		</server>

		<server>
			<id>harbor.contezza.nl</id>
			<username>${env.CNTZ_DOCKER_HARBOR_USER}</username>
			<password>${env.CNTZ_DOCKER_HARBOR_PASSWD}</password>
			<configuration>
				<email>beheer@contezza.nl</email>
			</configuration>
		</server>

	</servers>

	<profiles>
		<profile>
			<id>default</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<repositories>
				<repository>
					<id>cntz-alfresco-public</id>
					<url>https://nexus.contezza.nl/repository/alfresco-public/</url>
				</repository>
				<repository>
					<id>cntz-alfresco-private</id>
					<url>https://nexus.contezza.nl/repository/alfresco-private/</url>
				</repository>
				<repository>
					<id>cntz-activiti-private</id>
					<url>https://nexus.contezza.nl/repository/activiti-private-repository/</url>
				</repository>
				<repository>
					<id>cntz-alfresco-public-snapshots</id>
					<url>https://nexus.contezza.nl/repository/alfresco-public-snapshots/</url>
					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>daily</updatePolicy>
					</snapshots>
				</repository>
				<repository>
					<id>cntz-activiti-public</id>
					<url>https://nexus.contezza.nl/repository/activiti-public/</url>
				</repository>
				<repository>
					<id>secret-items</id>
					<url>https://nexus.contezza.nl/repository/secret-items/</url>
				</repository>
				<repository>
					<id>mvnrepository</id>
					<url>https://nexus.contezza.nl/repository/mvnrepository/</url>
				</repository>
				<repository>
					<id>thirdparty</id>
					<url>https://nexus.contezza.nl/repository/thirdparty/</url>
				</repository>
				<repository>
					<id>releases</id>
					<url>https://nexus.contezza.nl/repository/releases/</url>
				</repository>
				<repository>
					<id>snapshots</id>
					<url>https://nexus.contezza.nl/repository/snapshots/</url>
					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>daily</updatePolicy>
					</snapshots>
				</repository>
				<repository>
					<id>alfresco-public</id>
					<url>https://artifacts.alfresco.com/nexus/content/groups/public</url>
				</repository>
				<repository>
					<id>alfresco-public-snapshots</id>
					<url>https://artifacts.alfresco.com/nexus/content/groups/public-snapshots</url>
				</repository>
			</repositories>
		</profile>
	</profiles>

</settings>
